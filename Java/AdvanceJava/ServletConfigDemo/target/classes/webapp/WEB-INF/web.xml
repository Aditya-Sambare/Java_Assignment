<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>LoginPage.html</welcome-file>
    <welcome-file>default.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>WelcomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/logintoWelcome</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>bye</servlet-name>
    <servlet-class>ByeServlet</servlet-class>
    <init-param>
      <param-name>username</param-name>
      <param-value>Aditya</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>bye</servlet-name>
    <url-pattern>/byeservlet</url-pattern>
  </servlet-mapping>
</web-app>
