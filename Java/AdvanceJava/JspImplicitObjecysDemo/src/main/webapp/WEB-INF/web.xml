<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>Register.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>jayjsp</servlet-name>
    <jsp-file>/Greeting.jsp</jsp-file>

    <init-param>
      <param-name>configmessage</param-name>
      <param-value>"this is config message"</param-value>
    </init-param>

  </servlet>

  <servlet-mapping>
    <servlet-name>jayjsp</servlet-name>
    <url-pattern>/Greeting.jsp</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>applicationmessage</param-name>
    <param-value>"this is application message"</param-value>
  </context-param>

</web-app>
